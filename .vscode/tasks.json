{
	"version": "2.0.0",
	"tasks": [
        {
            "label": "C++: Build with QuantLib G++ 11",
            "type": "shell",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I/opt/homebrew/Cellar/boost/1.86.0/include",
                "-I/Users/francis/local/include",
                "-I${fileDirname}",
                "-isystem /opt/homebrew/include",
                "-std=c++11",
                "${fileDirname}/*.cpp",
                "-o",
                "${fileDirname}/bin",
                "-L/Users/francis/local/lib",
                "-L/opt/homebrew/Cellar/boost/1.86.0/lib",
                "-lQuantLib"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "new"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "detail": "Build generated by the debugger."
        },
